{
  "name": "webeye",
  "version": "1.0.0",
  "description": "URL personalisation and shortener with tracking capabilities",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "server": "nodemon app.js --ignore client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "./node_modules/.bin/eslint . --ext .js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Team-Probably/WebEye.git"
  },
