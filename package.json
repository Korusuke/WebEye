  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Team-Probabaly/WebEye/issues"
  },
  "homepage": "https://github.com/Team-Probabaly/WebEye#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cookie-parser": "^1.4.4",
    "crypto-js": "^3.1.9-1",
    "express": "^4.17.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongodb": "^3.3.1",
    "mongoose": "^5.5.10",
    "uuid": "^3.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "nodemon": "^1.19.0",
    "react": "^16.8.6"
  }
}
